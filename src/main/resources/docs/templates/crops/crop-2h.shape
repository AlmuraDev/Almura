bounds:
    use-vanilla-collision: false
    collision-box: "0 0 0 0 0 0"
    use-vanilla-wireframe: false
    wireframe-box: ".25 0 .25 .75 2 .75"
    use-vanilla-render: false
    render-box: ".25 0 .25 .75 2 .75"
shapes:
  - coordinates: |
      1 0 1 
      1 2 1
      0 2 0
      0 0 0
    texture: 0
  - coordinates: |
      1 0 0
      1 2 0
      0 2 1
      0 0 1
    texture: 0
