bounds:
    use-vanilla-collision: false
    collision-box: "0 0 0 0 0 0"
    use-vanilla-wireframe: false
    wireframe-box: ".25 0 .25 .75 3 .75"
    use-vanilla-block-bounds: false
    block-bounds: ".25 0 .25 .75 3 .75"
shapes:
  - coordinates: |
      1 0 1 
      1 3 1
      0 3 0
      0 0 0
    texture: 0
  - coordinates: |
      1 0 0
      1 3 0
      0 3 1
      0 0 1
    texture: 0
